{
  "name": "boilerplate",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run tsc-dev-server && node --require esm .next/transpiled-server-dev",
    "build": "next build && tsc -p ./tsconfig.server.json",
    "start": "NODE_ENV=production node --require esm .next/transpiled-server",
    "tsc": "tsc -p ./ --noEmit",
    "tsc-dev-server": "tsc -p ./tsconfig.server.json --outDir .next/transpiled-server-dev/",
    "lint": "tslint -p ./ -c tslint.json -t verbose",
    "jest": "jest",
    "test:watch": "jest --watch",
    "test": "npm run lint && npm run tsc && npm run jest && npm run size",
    "analyze": "ANALYZE=1 next build",
    "size": "npm run build && size-limit"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@zeit/next-typescript": "^1.1.1",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.3",
    "es6-promise": "^4.2.5",
    "esm": "^3.2.1",
    "express": "^4.16.4",
    "isomorphic-cookie": "^1.2.4",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.0",
    "jss": "^9.8.7",
    "lodash": "^4.17.10",
    "next": "^7.0.2",
    "next-redux-wrapper": "^2.1.0",
    "react": "^16.7.0",
    "react-braintree-fields": "^1.1.0",
    "react-dom": "^16.7.0",
    "react-jss": "^8.6.1",
    "react-redux": "6.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2",
    "reselect": "^4.0.0",
    "styled-jsx": "^3.2.0",
    "typesafe-actions": "^2.2.0",
    "utility-types": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/braintree-web": "^3.6.4",
    "@types/classnames": "^2.2.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.14",
    "@types/js-cookie": "^2.1.0",
    "@types/loadjs": "^3.5.0",
    "@types/lodash": "^4.14.120",
    "@types/next": "^7.0.8",
    "@types/next-redux-wrapper": "^2.0.2",
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.11",
    "@types/react-jss": "^8.6.1",
    "@types/react-redux": "^6.0.13",
    "@types/styled-jsx": "^2.2.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-plugin-inline-react-svg": "^1.0.0",
    "babel-plugin-module-resolver": "^3.1.3",
    "jest": "^23.5.0",
    "redux-devtools-extension": "^2.13.8",
    "size-limit": "^0.21.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.2.2",
    "webpack-bundle-analyzer": "^3.0.3"
  },
  "private": true
}
